{
  "name": "handle-written",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "test": "jest",
    "commit": "git add . && git-cz && git push origin master"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "vue": "^3.2.16"
  },
  "devDependencies": {
    "commitizen": "^4.2.4",
    "@vitejs/plugin-vue": "^1.9.2",
    "@types/jest": "^27.0.2",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "vite": "^2.6.0",
    "vue-tsc": "^0.3.0"
  }
}
